<div id="selectorsConverter">
	{{input id="source" type="text" placeholder="Type selector here..." value=souceSelector key-up="onSourceSelectorChanged"}}
	<ul id="target">
		<li>
			<label>Scss: </label>
			{{targetScss}}
		</li>
		<li>
			<label>Css: </label>
			{{css-validator value=targetCss}}
		</li>
		<li>
			<label>XPath: </label>
			{{xpath-validator value=targetXPath}}
		</li>
	</ul>
</div>