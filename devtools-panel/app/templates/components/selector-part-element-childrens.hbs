{{#if partElement.isExpanded}}
	{{#each partElement.children as |childElement|}}
		{{selector-part-element isChild=true partElement=childElement onAttributeToggle=(action "onAttributeToggle" childElement.part) onSelected=(action "onSelected")}}
		{{selector-part-element-childrens partElement=childElement onAttributeToggle=(action "onAttributeToggle" childElement.part) onSelected=(action "onSelected")}}
	{{/each}}
{{/if}}
