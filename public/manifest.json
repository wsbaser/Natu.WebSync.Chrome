{
  "manifest_version": 2,
  "author": "wsbaser",
  "name": "Natu SynchronizeIt",
  "version": "0.0.1",
  "description": "Browser extension to synchronize web page in Chrome and it's Page Object in Visual Studio.",
  "content_security_policy": "script-src 'self' ; object-src 'self'",
  "icons": {
    "64": "icons/icon64.png"
  }, 
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "devtools_page": "./devtools.html",
  "permissions": [ "tabs", "<all_urls>" ]
}